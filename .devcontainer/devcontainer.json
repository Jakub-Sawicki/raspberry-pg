{
    "name": "ROS2 Slam Dev",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "ros2_dev",
    "workspaceFolder": "/ros2_ws",
    "extensions": [
      "ms-iot.vscode-ros",
      "ms-vscode.cpptools",
      "ms-python.python"
    ],
    "settings": {
      "terminal.integrated.shell.linux": "/bin/bash"
    },
    "postCreateCommand": "colcon build; source install/setup.bash",
    "runArgs": [
      "--env=DISPLAY",
    ]
  }
  